package in.ashokit.beans;

public class WellWisher2
{
public static void wish(Greeting greeting)
{
	greeting.greet();
}
	public static void main(String[] args) 
	{
		wish(new Greeting() {
			@Override
			public void greet()
			{
				System.out.println("Hello, Good Morning....!!!!");
			}
		});
		
		wish(new Greeting() {
			@Override
			public void greet() {
				System.out.println("Namaskar....!!!!");
			}
		});
	}
}
